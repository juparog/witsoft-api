
# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 2
end_of_line=LF
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
max_line_length = off
insert_final_newline = false
trim_trailing_whitespace = false

[*.yml]
indent_style=espacio
indent_size = 2

[*.json]
indent_style=espacio
indent_size = 2
